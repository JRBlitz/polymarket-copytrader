{
  "name": "polymarket-copytrader",
  "version": "1.0.0",
  "description": "High-performance copy trading system for Polymarket",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev": "vite",
    "dev:app": "concurrently \"vite\" \"npm:electron\"",
    "build": "tsc && vite build",
    "build:electron": "esbuild electron/main/index.ts --bundle --platform=node --format=cjs --external:keytar --external:electron --outfile=dist-electron/main/index.js",
    "build:preload": "esbuild electron/preload/index.ts --bundle --platform=node --format=cjs --external:electron --outfile=dist-electron/preload/index.js",
    "build:all": "npm run build && npm run build:electron && npm run build:preload",
    "build:backend": "tsc --project tsconfig.backend.json",
    "preview": "vite preview",
    "electron": "wait-on tcp:5173 && cross-env IS_DEV=true electron .",
    "electron:pack": "npm run build:all && electron-builder --dir",
    "electron:dist": "npm run build:all && electron-builder",
    "dist": "npm run build:all && electron-builder --publish=never",
    "dist:win": "npm run build:all && electron-builder --win --publish=never",
    "dist:mac": "npm run build:all && electron-builder --mac --publish=never",
    "dist:linux": "npm run build:all && electron-builder --linux --publish=never",
    "postinstall": "electron-builder install-app-deps",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "start:backend": "ts-node src/backend/server.ts",
    "dev:backend": "nodemon --exec ts-node src/backend/server.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "keytar": "^7.9.0",
    "@polymarket/clob-client": "^2.1.0",
    "electron-updater": "^6.1.7",
    "ethers": "^6.8.1",
    "express-rate-limit": "^7.1.5",
    "fastify": "^4.24.3",
    "fastify-cors": "^6.1.0",
    "fastify-metrics": "^10.6.0",
    "fastify-rate-limit": "^5.9.0",
    "fastify-websocket": "^4.3.0",
    "helmet": "^8.1.0",
    "kafkajs": "^2.2.4",
    "lru-cache": "^10.1.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "prom-client": "^15.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.15",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.8.10",
    "@types/pg": "^8.10.7",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "electron-builder": "^24.6.4",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.4.31",
    "postcss-import": "^16.1.1",
    "tailwindcss": "^3.3.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^0.34.6",
    "wait-on": "^7.2.0",
    "concurrently": "^8.2.2",
    "esbuild": "^0.23.0"
  },
  "keywords": [
    "polymarket",
    "copy-trading",
    "trading-bot",
    "defi",
    "ethereum",
    "polygon",
    "high-performance",
    "real-time"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JRBlitz/polymarket-copytrader.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
